# Production Frontend Configuration
REACT_APP_API_URL=https://tradeai.gonxt.tech/api
REACT_APP_SOCKET_URL=https://tradeai.gonxt.tech
REACT_APP_AI_API_URL=https://tradeai.gonxt.tech/ai
REACT_APP_MONITORING_URL=https://tradeai.gonxt.tech/monitoring

# Environment
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=2.0.0

# Multi-tenant Configuration
REACT_APP_COMPANY_ISOLATION=true
REACT_APP_DEFAULT_COMPANY=GONXT

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_REAL_TIME_UPDATES=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Build Configuration
GENERATE_SOURCEMAP=false
REACT_APP_BUILD_DATE=2025-09-12
REACT_APP_CACHEBUST=1

# Security
REACT_APP_SECURE_COOKIES=true
REACT_APP_HTTPS_ONLY=true

# Performance
REACT_APP_LAZY_LOADING=true
REACT_APP_CODE_SPLITTING=true

# Logging
REACT_APP_LOG_LEVEL=error
REACT_APP_ENABLE_CONSOLE_LOGS=false

# Analytics
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_ERROR_TRACKING=true